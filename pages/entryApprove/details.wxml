<view>
  <view class='list-item'>
    <label class='list-item-lb-title'>接单编号：</label>
    <view class='list-item-lb-value'>{{obj.INNER_NO}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>状态：</label>
    <view class='list-item-lb-value'>{{array[obj.STATUS]}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>接单日期：</label>
    <view class='list-item-lb-value'>{{obj.RCV_DATE}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>委托客户：</label>
    <view class='list-item-lb-value'>{{obj.COMPANY}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>结费周期：</label>
    <view class='list-item-lb-value'>{{obj.BILLING_CYCLE}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>提货单位：</label>
    <view class='list-item-lb-value'>{{obj.DELIVERY_NAME_S}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>送货单位：</label>
    <view class='list-item-lb-value'>{{obj.DELIVERY_NAME_R}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>要求报关时间：</label>
    <view class='list-item-lb-value'>{{obj.DEMAND_DATE}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>{{i_e_flag=="I"?"进口口岸":"出口口岸"}}：</label>
    <view class='list-item-lb-value'>{{obj.I_E_PORT}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>备案号：</label>
    <view class='list-item-lb-value'>{{obj.MANUAL_NO}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>合同协议号：</label>
    <view class='list-item-lb-value'>{{obj.CONTR_NO}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>{{i_e_flag=="I"?"进口日期":"出口日期"}}：</label>
    <view class='list-item-lb-value'>{{obj.I_E_DATE}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>申报地海关：</label>
    <view class='list-item-lb-value'>{{obj.CUSTOM_MASTER}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>申报日期：</label>
    <view class='list-item-lb-value'>{{obj.D_DATE}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>收发货人：</label>
    <view class='list-item-lb-value'>{{obj.TRADE_NAME}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>消费使用单位：</label>
    <view class='list-item-lb-value'>{{obj.OWNER_NAME}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>申报单位：</label>
    <view class='list-item-lb-value'>{{obj.AGENT_CODE}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>运输方式：</label>
    <view class='list-item-lb-value'>{{obj.TRAF_MODE}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>运输工具名称：</label>
    <view class='list-item-lb-value'>{{obj.TRAF_NAME}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>航次号：</label>
    <view class='list-item-lb-value'>{{obj.VOYAGE_NO}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>提运单号：</label>
    <view class='list-item-lb-value'>{{obj.BILL_NO}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>监管方式：</label>
    <view class='list-item-lb-value'>{{obj.TRADE_MODE}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>征免性质：</label>
    <view class='list-item-lb-value'>{{obj.CUT_MODE}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>纳税单位：</label>
    <view class='list-item-lb-value'>{{obj.PAYMENT_MARK}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>许可证号：</label>
    <view class='list-item-lb-value'>{{obj.LICENSE_NO}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>{{i_e_flag=="I"?"启运国(地区)":"运抵国(地区)"}}：</label>
    <view class='list-item-lb-value'>{{obj.TRADE_COUNTRY}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>{{i_e_flag=="I"?"装货港":"指运港"}}：</label>
    <view class='list-item-lb-value'>{{obj.DISTINATE_PORT}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>{{i_e_flag=="I"?"境内目的地":"境内货源地"}}：</label>
    <view class='list-item-lb-value'>{{obj.DISTRICT_CODE}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>成交方式：</label>
    <view class='list-item-lb-value'>{{obj.TRANS_MODE}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>运费：</label>
    <view class='list-item-lb-value'>{{obj.FEE_MARK}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>保费：</label>
    <view class='list-item-lb-value'>{{obj.INSUR_MARK}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>杂费：</label>
    <view class='list-item-lb-value'>{{obj.OTHER_MARK}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>件数：</label>
    <view class='list-item-lb-value'>{{obj.PACK_NO}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>包装类型：</label>
    <view class='list-item-lb-value'>{{obj.WRAP_TYPE}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>毛重：</label>
    <view class='list-item-lb-value'>{{obj.GROSS_WT}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>净重：</label>
    <view class='list-item-lb-value'>{{obj.NET_WT}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>贸易国(地区)：</label>
    <view class='list-item-lb-value'>{{obj.TRADE_AREA_CODE}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>报关单类型：</label>
    <view class='list-item-lb-value'>{{obj.ENTRY_TYPE}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>集装箱数：</label>
    <view class='list-item-lb-value'>{{obj.CONTAINERS}}</view>
  </view>
  <view class='list-item'>
    <label class='list-item-lb-title'>随附单证：</label>
    <view class='list-item-lb-value'>{{obj.CERT_MARK}}</view>
  </view>
  <view class='list-item' style='margin-bottom:20px;'>
    <label class='list-item-lb-title'>备注：</label>
    <view class='list-item-lb-value'>{{obj.NOTE}}</view>
  </view>
  <view class='search-btns' bindtap='ShowAudit'>
    <button class="search-btn" formType="submit">审批通过</button>
  </view>
  <modal hidden="{{hiddenmodalput}}" data-id="{{obj.SEQ_NO}}" confirm-text="确认" cancel-text="取消" bindcancel="cancel" bindconfirm="reviewResult">
  <view class='search-item'>
    你确认要审批吗
  </view>
</modal>
</view>