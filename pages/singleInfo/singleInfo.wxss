 page {
  font-size: 14px; 
  height: 100%; 
  width: 100%;
  overflow: hidden;
} 

.logo-img {
  width: 100%;
  height: 40%;
}

._container {
  height: 100%;
  width: 100%;
}

.logo {
  display: flex;
  align-items: center;
  justify-content: center;
  border: 0;
}

.err-tips {
  background-color: #e64340;
  text-align: center;
  color: #fff;
  height: 30px;
  line-height: 30px;
  position: absolute;
}

.load {
  height: 80%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.info-item {
  display: flex;
  justify-content: flex-start;
  align-items: flex-end;
  margin-left: 5%;
  width: 90%;
  margin-bottom: 15px;
  background-color: #e7f1f7;
  border-radius: 5px;
}
.info-item1 {
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  margin-left: 5%;
  width: 90%;
  margin-bottom: 15px;
  background-color: #e7f1f7;
  border-radius: 5px;
}
.info-item-content{
 display: flex; 
}
.input-style {
  padding-top: 20px;
}

.info-item-input {
  padding-left: 10px;
  border: 0px solid #dcdcdc;
  color: #333;
  font-size: 16px;
  font-family: "microsoft yahei";
  height: 45px;
  width: 85%;
}

.info-item-input1 {
  padding-left: 10px;
