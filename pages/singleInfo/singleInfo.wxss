  border-bottom: 1px solid #f4f4f4;
}

.s-title {
  font-size: 16px;
}

.s-content {
  font-size: 15px;
  color: #c1c1c1;
  padding-left: 20px;
}

.s-search-btn {
  height: 45px;
  border-radius: 5px;
  padding-top: 10px;
  font: 16px "microsoft yahei";
